{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": "dist/scripts",
        "includePattern": ".+\\.js(doc)?$"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "template": "templates/default",
      "encoding": "utf8",
      "destination": "./reports/jsdoc/",
      "recurse": true
  }
}
