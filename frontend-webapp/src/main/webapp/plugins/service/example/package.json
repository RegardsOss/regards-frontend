{
  "name": "example-plugin",
  "version": "2.0.0-SNAPSHOT",
  "description": "REGARDS frontend criteria plugin for string attributes",
  "private": true,
  "scripts": {
    "build": "webpack --progress --config webpack.prod.config.js",
    "build:watch": "nodemon -e js,jsx --ignore \"target\" --exec \"webpack --progress --config webpack.dev.config.js\"",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register",
    "test:mocha": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend - Plugin service example tests'",
    "test:watch": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --watch",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint .storybook src tests --ext .js,.jsx",
    "lint:fix": "eslint --fix .storybook src tests --ext .js,.jsx",
    "storybook:start": "start-storybook -p 6006 --dont-track",
    "storybook:build": "build-storybook -o reports/storybook"
  },
  "author": "CS-SI",
  "license": "GPL-3.0",
  "dependencies": {
    "lodash": "^4.17.4",
    "material-ui": "^0.18.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "~2.3.0",
    "react-redux": "~5.0.4",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@regardsoss/webpack-config-front": "../../../webpack-config-front",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.1.0",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "~6.24.1",
    "babel-runtime": "~6.23.0",
    "chai": "~3.5.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.8.2",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "~3.19.0",
    "eslint-config-airbnb": "~15.0.1",
    "eslint-loader": "~1.8.0",
    "eslint-plugin-import": "~2.3.0",
    "eslint-plugin-jsx-a11y": "~5.0.3",
    "eslint-plugin-lodash": "^2.3.5",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "~7.1.0",
    "eslint-plugin-react-perf": "^2.0.4",
    "mocha": "~3.4.1",
    "mocha-loader": "~1.1.1",
    "mocha-sonar-reporter": "~0.1.6",
    "mocha-webpack": "^1.0.0-beta.1",
    "mochawesome": "^2.2.1",
    "nock": "~9.0.13",
    "node-http-proxy": "^0.2.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "~11.0.2",
    "parse-body": "^1.0.3",
    "prop-types": "^15.5.10",
    "react-hot-loader": "~3.0.0-beta.3",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "~3.4.0",
    "redux-devtools-dock-monitor": "~1.1.2",
    "redux-devtools-log-monitor": "~1.3.0",
    "redux-mock-store": "~1.2.3",
    "sinon": "^2.3.5",
    "source-map-loader": "~0.2.1",
    "source-map-support": "^0.4.15",
    "webpack": "3.3.0",
    "xunit-file": "~1.0.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "~3.10.7"
  },
  "config": {
    "mocha-sonar-reporter": {
      "outputfile": "./reports/coverage/TEST-xunit.xml"
    }
  }
}