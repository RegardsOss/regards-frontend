{
  "name": "enumerated",
  "version": "1.0.0",
  "description": "Enumerated values criteria plugin for Regards search forms",
  "private": true,
  "scripts": {
    "build:production": "webpack --progress --config webpack.prod.config.js",
    "build:dev": "webpack --progress --config webpack.dev.config.js",
    "build:watch": "nodemon -e js,jsx --ignore \"target\" --exec \"webpack --progress --config webpack.dev.config.js\"",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register",
    "test:mocha": "node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend - Plugin service example tests'",
    "test:watch": "node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --watch",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint src tests --ext .js,.jsx",
    "lint:fix": "eslint --fix  src tests --ext .js,.jsx"
  },
  "author": "RaphaÃ«l Mechali",
  "license": "GPL-3.0",
  "dependencies": {
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "~2.4.0",
    "react-redux": "~5.0.7",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@regardsoss/webpack-config-front": "file:../../../webpack-config-front",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.2",
    "babel-loader": "~7.1.3",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "~6.26.0",
    "babel-runtime": "~6.26.0",
    "chai": "~4.1.2",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-loader": "~2.0.0",
    "eslint-plugin-import": "~2.9.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-lodash": "^2.6.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "~7.7.0",
    "eslint-plugin-react-perf": "^2.0.8",
    "mocha": "~5.0.1",
    "mocha-loader": "~1.1.3",
    "mocha-sonar-reporter": "~0.1.6",
    "mocha-webpack": "^1.0.1",
    "mochawesome": "^3.0.2",
    "nodemon": "^1.15.1",
    "nyc": "~11.4.1",
    "parse-body": "^1.0.3",
    "prop-types": "^15.6.1",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "~3.4.1",
    "redux-devtools-dock-monitor": "~1.1.3",
    "redux-devtools-log-monitor": "~1.4.0",
    "redux-mock-store": "~1.5.1",
    "sinon": "^4.4.2",
    "source-map-loader": "~0.2.3",
    "source-map-support": "^0.5.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "~2.11.1",
    "xunit-file": "~1.0.0"
  },
  "engines": {
    "node": ">=6.11.2",
    "npm": ">=3.10.10"
  },
