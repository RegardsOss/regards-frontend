{
  "name": "numerical-criteria-plugin",
  "version": "1.0.0",
  "description": "REGARDS frontend criteria plugin for numerical attributes",
  "private": true,
  "scripts": {
    "build": "webpack --progress --config webpack.config.js",
    "postbuild": "mkdir -p ../../../build/plugins/criterion/numerical/ && cp target/build/plugin.js ../../../build/plugins/criterion/numerical/",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register",
    "test:mocha": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend - Plugin Numerical Criterion tests'",
    "test:watch": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --watch",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint .storybook src tests --ext .js,.jsx",
    "lint:fix": "eslint --fix .storybook src --ext .js,.jsx",
    "storybook:start": "start-storybook -p 6006 --dont-track",
    "storybook:build": "build-storybook -o reports/storybook"
  },
  "author": "CS-SI",
  "license": "GPLv3",
  "dependencies": {
    "lodash": "^4.17.4",
    "material-ui": "^0.16.6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-intl": "~2.2.2",
    "react-redux": "~5.0.1",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "~1.7.1",
    "autoprefixer": "^6.6.0",
    "babel-cli": "~6.18.0",
    "babel-core": "~6.21.0",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.4",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-transform-class-properties": "~6.19.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "~6.18.0",
    "babel-runtime": "~6.20.0",
    "chai": "~3.5.0",
    "clean-webpack-plugin": "~0.1.9",
    "css-loader": "~0.26.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.7.0",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "~3.12.2",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-loader": "~1.6.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.8.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "fs-extra": "^1.0.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "json-loader": "~0.5.4",
    "mocha": "~3.2.0",
    "mocha-loader": "~1.0.0",
    "mocha-sonar-reporter": "~0.1.6",
    "mocha-webpack": "~0.7.0",
    "mochawesome": "^2.0.0",
    "nock": "~9.0.2",
    "nyc": "~10.0.0",
    "postcss-loader": "~1.2.1",
    "react-addons-test-utils": "~15.4.1",
    "react-hot-loader": "~3.0.0-beta.3",
    "redux-devtools": "~3.3.1",
    "redux-devtools-dock-monitor": "~1.1.1",
    "redux-devtools-log-monitor": "~1.1.1",
    "redux-mock-store": "~1.2.0",
    "sinon": "^1.17.7",
    "source-map-loader": "~0.1.5",
    "source-map-support": "^0.4.8",
    "storybook-addon-material-ui": "^0.7.4",
    "style-loader": "~0.13.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.14.0",
    "webpack-merge": "~1.1.2",
    "webpack-node-externals": "~1.5.4",
    "why-did-you-update": "0.0.8",
    "xunit-file": "~1.0.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "~3.10.7"
  },
  "config": {
    "mocha-sonar-reporter": {
      "outputfile": "./reports/coverage/TEST-xunit.xml"
    }
  }
}
