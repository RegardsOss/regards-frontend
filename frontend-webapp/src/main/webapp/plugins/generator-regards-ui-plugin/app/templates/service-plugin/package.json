{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "private": true,
  "scripts": {
    "build": "webpack --progress --config webpack.config.js",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register",
    "test:mocha": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend - Plugin String Criterion tests'",
    "test:watch": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"tests/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --watch",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint .storybook src tests --ext .js,.jsx",
    "lint:fix": "eslint --fix .storybook src tests --ext .js,.jsx",
    "storybook:start": "start-storybook -p 6006 --dont-track",
    "storybook:build": "build-storybook -o reports/storybook"
  },
  "author": "<%= author %>",
  "license": "<%= licence %>",
  "dependencies": {
    "lodash": "^4.17.4",
    "material-ui": "^0.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-intl": "~2.2.3",
    "react-redux": "~5.0.4",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "~1.7.1",
    "autoprefixer": "^6.7.6",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.24.1",
    "babel-eslint": "~7.2.2",
    "babel-loader": "~6.4.1",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "~6.24.1",
    "babel-runtime": "~6.23.0",
    "chai": "~3.5.0",
    "clean-webpack-plugin": "~0.1.16",
    "css-loader": "~0.28.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.8.2",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "~3.19.0",
    "eslint-config-airbnb": "~14.1.0",
    "eslint-loader": "~1.7.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~4.0.0",
    "eslint-plugin-react": "~6.10.3",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.11.1",
    "fs-extra": "^2.1.2",
    "happypack": "^3.0.3",
    "json-loader": "~0.5.4",
    "mocha": "~3.2.0",
    "mocha-loader": "~1.1.1",
    "mocha-sonar-reporter": "~0.1.6",
    "mocha-webpack": "^1.0.0-beta.1",
    "mochawesome": "^2.0.5",
    "nock": "~9.0.13",
    "node-http-proxy": "^0.2.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "~10.2.2",
    "parse-body": "^1.0.3",
    "postcss-loader": "~1.3.3",
    "react-addons-test-utils": "~15.4.2",
    "react-hot-loader": "~3.0.0-beta.3",
    "redux-devtools": "~3.3.2",
    "redux-devtools-dock-monitor": "~1.1.1",
    "redux-devtools-log-monitor": "~1.2.0",
    "redux-mock-store": "~1.2.3",
    "sinon": "^2.1.0",
    "source-map-loader": "~0.2.1",
    "source-map-support": "^0.4.14",
    "storybook-addon-material-ui": "^0.7.7",
    "style-loader": "~0.16.1",
    "url-loader": "~0.5.8",
    "webpack": "2.4.1",
    "webpack-dev-server": "~2.4.2",
    "webpack-merge": "~4.1.0",
    "webpack-node-externals": "~1.5.4",
    "why-did-you-update": "0.0.8",
    "xunit-file": "~1.0.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "~3.10.7"
  },
  "config": {
    "mocha-sonar-reporter": {
      "outputfile": "./reports/coverage/TEST-xunit.xml"
    }
  }
}
