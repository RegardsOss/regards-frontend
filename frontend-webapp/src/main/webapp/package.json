{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "private": true,
  "scripts": {
    "bootstrap": "./scripts/bootstrap.sh",
    "build:dll": "webpack --progress --config webpack.dll.config.js --display-error-details",
    "build:production": "webpack --progress --config webpack.prod.config.js --display-error-details",
    "prestart": "npm run lint:fix || true",
    "start": "npm run start:withpreprod",
    "start:withpreprod": "webpack-dev-server --progress --hot --config webpack.dev.preprod.config.js",
    "start:mock-client": "webpack-dev-server --progress --hot --config webpack.dev.mock.config.js",
    "start:mock-server-facade": "nodemon --watch ./mocks --ignore '*.temp.js' ./mocks/mock-server-facade.js",
    "start:mock-server-json": "nodemon --watch ./mocks --ignore '*.temp.js' ./mocks/mock-server-json.js",
    "start:withmock": "npm-run-all --parallel start:mock-*",
    "pretest": "npm run lint:fix || true",
    "postinstall": "npm run build:dll",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --timeout 5000 --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register",
    "test:mocha": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/mocha-webpack --webpack-config webpack.test.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --timeout 20000 --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend tests'",
    "test:watch": "mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"web_modules/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --watch",
    "storybook:start": "start-storybook -p 6006 --dont-track",
    "storybook:build": "build-storybook -o reports/storybook",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint web_modules .storybook src --ext .js,.jsx",
    "lint:fix": "eslint --fix web_modules .storybook src --ext .js,.jsx"
  },
