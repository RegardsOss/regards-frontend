{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "private": true,
  "scripts": {
    "bootstrap": "./scripts/bootstrap.sh",
    "build:production": "webpack --progress --config webpack.prod.config.js",
    "prestart": "npm run lint:fix || true",
    "start": "webpack-dev-server --progress --hot --config webpack.dev.config.js",
    "pretest": "npm run lint:fix || true",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc mocha-webpack --timeout 5000 --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register  --reporter mocha-sonar-reporter",
    "test:mocha": "mocha-webpack --webpack-config webpack.test.config.js \"**/@(tests|web_modules)/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend tests'",
    "test:watch": "mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"web_modules/**/*.test.js{,x}\" --recursive --require source-map-support/register --inline-diffs --watch",
    "storybook:start": "start-storybook -p 6006 --dont-track",
    "storybook:build": "build-storybook -o reports/storybook",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint web_modules .storybook src --ext .js,.jsx",
    "lint:fix": "eslint --fix web_modules .storybook src --ext .js,.jsx"
  },
  "author": "CS-SI",
  "license": "GPLv3",
  "dependencies": {
    "@regardsoss/access-rights": "0.0.1",
    "@regardsoss/admin": "0.0.1",
