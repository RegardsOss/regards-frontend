{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "main": "dist/src/main.js",
  "private": true,
  "scripts": {
    "bootstrap": "./scripts/bootstrap.sh",
    "postinstall": "npm run typings:reset",
    "prebuild": "npm run bootstrap && npm install && npm run test:coverage",
    "build:production": "npm run prebuild && webpack --progress --config webpack.prod.config.js",
    "start": "node --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/webpack-dev-server --progress --hot --config webpack.dev.config.js",
    "pretest": "(npm run lint --silent || true)",
    "test": "npm run test:mocha --silent",
    "test:coverage": "nyc mocha-webpack --webpack-config webpack.coverage.config.js \"**/@(tests|web_modules)/**/*.test.\\?(ts|tsx)\" --recursive  --require source-map-support/register --reporter mocha-sonar-reporter || true",
    "test:mocha": "mocha-webpack --webpack-config webpack.test.config.js \"**/@(tests|web_modules)/**/*.test.\\?(ts|tsx)\" --recursive --require source-map-support/register --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend tests'",
    "test:watch": "mocha-webpack --webpack-config webpack.test.config.js \"**/@(tests|web_modules)/**/*.test.\\?(ts|tsx)\" --recursive --require source-map-support/register --inline-diffs --watch",
    "typings:reset": "rm -rf typings && typings install",
    "typings": "typings install --global --save",
    "storybook:start": "start-storybook -p 6006 --dont-track",
    "storybook:build": "build-storybook -o reports/storybook",
    "jsdoc": "jsdoc -c jsdoc.conf.json",
    "presitemap": "webpack --config webpack.sitemap.config.js --progress",
    "sitemap": "node build/sitemap.js",
    "lint": "tslint -c tslint.json 'src/{,**/}*.ts' 'src/{,**/}*.tsx' 'web_modules/{,**/}*.ts' 'web_modules/{,**/}*.tsx' -e '**test.tsx{0,1}' -e 'web_modules/**/node_modules/**' "
  },
  "author": "CS-SI",
  "license": "ISC",
  "dependencies": {
    "@regardsoss/access-rights": "0.0.1",
    "@regardsoss/admin": "0.0.1",
    "@regardsoss/admin-data-management": "0.0.1",
    "@regardsoss/admin-project-management": "0.0.1",
    "@regardsoss/api": "0.0.1",
    "@regardsoss/authentication": "0.0.1",
    "@regardsoss/components": "0.0.1",
    "@regardsoss/display-control": "0.0.1",
    "@regardsoss/i18n": "0.0.1",
    "@regardsoss/injector": "0.0.1",
    "@regardsoss/models": "0.0.1",
    "@regardsoss/plugins": "0.0.1",
    "@regardsoss/portal": "0.0.1",
    "@regardsoss/some-pckg": "0.0.1",
    "@regardsoss/store": "0.0.1",
    "@regardsoss/store-utils": "0.0.1",
    "@regardsoss/theme": "0.0.1",
    "flux-standard-action": "^0.6.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.4",
    "moment": "^2.15.1",
    "normalizr": "^2.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-resizable": "^1.4.2",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-api-middleware": "^1.0.2",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "scriptjs": "^2.5.8",
    "sockjs-client": "^1.1.1",
    "stompjs": "^2.3.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.16.1",
    "@kadira/storybook-addon-knobs": "^1.2.1",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.1",
    "mocha-sonar-reporter": "^0.1.6",
    "mocha-webpack": "^0.6.0",
    "mochawesome": "^1.5.1",
    "nock": "^8.0.0",
    "nyc": "^7.1.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.2.0",
    "sinon": "^1.17.4",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-eslint-rules": "^1.3.0",
    "typescript": "^2.1.0-dev.20160919",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.3.3",
    "xunit-file": "^1.0.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "^3.10.7"
  },
  "nyc": {
    "exclude": [
      ".tmp",
      "*.d.ts",
      "material-ui"
    ],
    "require": [
      "babel-core/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts",
      ".tsx",
      ".js"
    ],
    "reporter": [
      "lcov"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": false
  },
  "config": {
    "mocha-sonar-reporter": {
      "outputfile": "./reports/coverage/TEST-xunit.xml"
    }
  }
}
