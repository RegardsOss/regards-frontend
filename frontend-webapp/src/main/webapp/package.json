{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "private": true,
  "scripts": {
    "bootstrap": "./scripts/bootstrap.sh && npm prune",
    "build:dll": "webpack --progress --config webpack.dev.dll.config.js --display-error-details",
    "build:production-dll": "npm run build:production-core-dll && npm run build:production-coreoss-dll",
    "build:production-core-dll": "webpack --progress --config webpack.prod.core.dll.config.js --display-error-details",
    "build:production-coreoss-dll": "webpack --progress --config webpack.prod.coreoss.dll.config.js --display-error-details",
    "build:production": "webpack --progress --config webpack.prod.config.js --display-error-details",
    "build:plugins": "./scripts/buildPlugins.sh",
    "build:stats": "webpack --config webpack.dev.config.js --profile --json > reports/stats-build-dev.json",
    "prestart": "npm run lint:fix || true",
    "start": "npm run start:withpreprod",
    "start:local": "webpack-dev-server --progress --config webpack.dev.local.config.js",
    "start:mock-client": "webpack-dev-server --progress --config webpack.dev.mock.config.js",
    "start:mock-front-server": "nodemon --watch ./mocks/front --ignore '*.temp.js' ./mocks/front/server-main.js",
    "start:mock-server-json": "nodemon --watch ./mocks/json --ignore '*.temp.js' ./mocks/json/mock-server-json.js",
    "start:withmock": "npm-run-all --parallel start:mock-client start:mock-server-json start:mock-front-server",
    "start:mock-proxy-server-vm-perf": "nodemon --watch ./mocks/proxy ./mocks/proxy/server-vm-perf.js",
    "start:mock-proxy-server-vm-preprod": "nodemon --watch ./mocks/proxy ./mocks/proxy/server-vm-preprod.js",
    "start:withmock-proxy-vm-preprod": "npm-run-all --parallel start:mock-client start:mock-proxy-server-vm-preprod",
    "start:withmock-proxy-vm-perf": "npm-run-all --parallel start:mock-client start:mock-proxy-server-vm-perf",
    "start:withpreprod": "webpack-dev-server --progress --config webpack.dev.preprod.config.js",
    "start:withvmperf": "webpack-dev-server --progress --config webpack.dev.vmperf.config.js",
    "start:copy-plugins": "./scripts/copyPlugins.sh",
    "poststart": "npm run start:copy-plugins",
    "pretest": "npm run lint:fix || true",
    "postinstall": "npm run build:dll && npm run build:production-dll",
    "test": "npm run test:mocha --silent",
    "test:coverage": "node --max_old_space_size=8192 ./node_modules/.bin/nyc node --max_old_space_size=8192 ./node_modules/.bin/mocha-webpack --timeout 30000 --webpack-config webpack.coverage.config.js \"@(tests|web_modules)/**/*.test.js{,x}\"",
    "test:mocha": "mocha-webpack --timeout 20000 --webpack-config webpack.test.config.js \"@(tests|web_modules)/**/*.test.js{,x}\" --inline-diffs --reporter mochawesome --reporter-options reportDir='./reports/mocha',reportName='Regards Frontend tests'",
    "test:watch": "mocha-webpack --timeout 5000 --webpack-config webpack.test.config.js \"web_modules/**/*.test.js{,x}\" --inline-diffs --watch",
    "storybook:start": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook -o reports/storybook",
    "esdoc": "esdoc -c esdoc.json",
    "lint": "eslint web_modules .storybook src --ext .js,.jsx",
    "lint:fix": "eslint --fix web_modules src --ext .js,.jsx"
  },
